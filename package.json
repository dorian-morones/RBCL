{
  "name": "rbcl",
  "version": "0.0.0-development",
  "description": "React UI component Library",
  "main": "./dist/index.js",
  "keywords": [
    "react",
    "component-library",
    "react-component",
    "ui"
  ],
  "homepage": "",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "prod": "npm run build rollup.config.prod.js",
    "dev": "npm run build rollup.config.dev.js",
    "no-watch": "echo 'Building RBC-UI' && npm run build rollup.config.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "storybook:export": "build-storybook",
    "generate": "node ./util/create-component",
    "prepublishOnly": "npm run prod",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "pre-push": "npm run test"
    }
  },
  "author": {
    "name": "Dorian Morones",
    "email": "dorian.morones@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dorian-morones/RBCL"
  },
  "bugs": {
    "url": "https://github.com/dorian-morones/RBCL"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.3.1"
  },
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/pluginutils": "^4.1.1",
    "@storybook/addon-controls": "^6.3.7",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@storybook/source-loader": "^6.3.7",
    "@storybook/theming": "^6.3.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.13",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "babel-preset-react-app": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "rollup": "^2.56.3",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass-loader": "^12.1.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@testing-library/user-event": "^13.2.1",
    "global-jsdom": "^8.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.0",
    "jsdom": "^17.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-element-to-jsx-string": "^14.3.2",
    "styled-components": "^5.3.1",
    "semantic-release": "^17.4.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
